<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>MediaFilesManager</id>
    <version>1.0.0</version>
    <title>MediaFilesManager</title>
    <authors>Felipe Nicoletto</authors>
    <owners>buildguest</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Simple cross platform plugin to access images and videos on Android and iOS</description>
    <copyright>Copyright © 2017</copyright>
    <dependencies>
      <group targetFramework="netstandard1.0">
        <dependency id="NETStandard.Library" version="1.6.1" />
      </group>
      <group targetFramework="MonoAndroid10">
        <dependency id="Xamarin.Android.Support.Compat" version="26.1.0.1"/>
      </group>
    </dependencies>
    <projectUrl>https://github.com/FelipeNicoletto/MediaFilesManager</projectUrl>
    <tags>xamarin dot-net ios android cross-platform plugin media files manager photos videos images access</tags>
  </metadata>
  <files>
    <!--Core-->
    <file src="MediaFilesManager\bin\Release\netstandard1.0\MediaFilesManager.*" target="lib\netstandard1.0" />

    <!--Xamarin.Android-->
    <file src="MediaFilesManager.Android\bin\Release\MediaFilesManager.*" target="lib\MonoAndroid80" />

    <!--Xamarin.iOS-->
    <file src="MediaFilesManager.iOS\bin\Release\MediaFilesManager.*" target="lib\Xamarin.iOS10" />

  </files>
</package>
